#!/usr/bin/env node

const main = require('../package/index');
const argv = require('minimist')(process.argv.slice(1), {
  default: {
    cli: true,
    cwd: process.cwd()
  },
  string: [
    's',
    't',
    'o'
  ],
  alias: {
    store: 's',
    theme: 't',
    output: 'o'
  }
});

main(argv);
