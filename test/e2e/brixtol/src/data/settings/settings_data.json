      "rewaxing_service": {
        "type": "rewaxing_service",
        "blocks": {
          "1597903961762": {
            "type": "text_block",
            "settings": {
              "title": "Send To Brixtol",
              "content": "<p>To get your waxed cotton jacket professionally rewaxed by Brixtol, do so by sending your garment to us here in Stockholm, Sweden.</p>",
              "auto_open": "data-open"
            }
          },
          "1597904296094": {
            "type": "text_block",
            "settings": {
              "title": "Fabric Treatment",
              "content": "<p>We rewax garments using our <a href=\"/products/reproof-spray\" title=\"Reproof Spray\">Reproof Spray. </a>Our wax is fully biodegradable, as PFC’s are extremely persistent in the environment, and free from fluorocarbons.</p>",
              "auto_open": "data-open"
            }
          },
          "1597904315489": {
            "type": "text_block",
            "settings": {
              "title": "Receive from Brixtol",
              "content": "<p>We will express ship your garment back to the address you used at checkout after the rewaxing treatment process has completed.</p>",
              "auto_open": "data-open"
            }
          }
        },
