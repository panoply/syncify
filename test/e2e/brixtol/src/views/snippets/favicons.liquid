<link
  data-spx-eval="false"
  rel="shortcut icon"
  href=" {{- settings.favicon_ico }}">

<link
  data-spx-eval="false"
  rel="apple-touch-icon"
  sizes="180x180"
  href=" {{- settings.favicon_apple_touch }}">

<link
  data-spx-eval="false"
  rel="icon"
  type="image/png"
  sizes="32x32"
  href=" {{- settings.favicon_32x }}">

<link
  data-spx-eval="false"
  rel="icon"
  type="image/png"
  sizes="16x16"
  href=" {{- settings.favicon_16x }}">

<link
  data-spx-eval="false"
  rel="mask-icon"
  href=" {{- settings.favicon_safari_pinned_tab }}"
  color=" {{- settings.favicon_safari_pinned_tab_color }}">

<meta
  data-spx-eval="false"
  name="theme-color"
  content=" {{- settings.favicon_theme_color }}">

<meta
  data-spx-eval="false"
  name="application-name"
  content=" {{- settings.favicon_application_name | upcase }}">

<meta
  data-spx-eval="false"
  name="apple-mobile-web-app-title"
  content=" {{- settings.favicon_apple_mobile_web_app_title }}">

<meta
  data-spx-eval="false"
  name="msapplication-TileColor"
  content=" {{- settings.favicon_msapplication_title_color | upcase }}">
